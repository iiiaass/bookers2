<div class="container px-5 px-sm-0">
  <div class="row">
    <div class="col-md-3">


<%= render 'users/info', user: current_user %>
<%= render 'users/list', user: @user, book: @book %>
    </div>



<div class="col-md-8 offset-md-1">
      <h2>Users</h2>
      <table class="table">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
      <% @users.each do |f| %>
      <tr>
        <td><%=image_tag f.get_profile_image(100,100) %></td>
        <td><%=f.name %></td>
        <td>
           <%=link_to "Show",user_path(f.id) %>
           <%end%>
        </td>
      </tr>

  </tbody>
</table>

    </div>